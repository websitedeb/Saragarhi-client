{
  "expo": {
    "name": "Saragarhi",
    "slug": "proj",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/light_icon.png",
    "scheme": "proj",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/light_icon.png",
        "backgroundColor": "#ed1c24"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/light_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ed1c24"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
